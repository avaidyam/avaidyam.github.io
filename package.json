{
  "name": "Aditya Vaidyam",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.21",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.21",
    "@docusaurus/preset-classic": "^2.0.0-beta.21",
    "@docusaurus/utils": "^2.0.0-beta.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "fix-config": "echo \"module.exports = require('./package.json').docusaurus\" > docusaurus.config.js",
    "prebuild": "npm run fix-config",
    "prestart": "npm run prebuild",
    "docusaurus": "docusaurus",
    "start": "docusaurus start --host 0.0.0.0 --no-open --poll",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "dev": "docusaurus start",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "babel": {
    "compact": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "loose": true,
          "corejs": "3",
          "modules": false,
          "exclude": [
            "transform-typeof-symbol"
          ]
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": false,
          "helpers": false,
          "regenerator": true,
          "useESModules": true
        }
      ],
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "docusaurus": {
    "title": "Aditya Vaidyam",
    "tagline": "Aditya Vaidyam.",
    "url": "https://aditya.vaidyam.me/",
    "favicon": "photo.png",
    "organizationName": "avaidyam",
    "projectName": "avaidyam.github.io",
    "baseUrl": "/",
    "onBrokenLinks": "error",
    "onDuplicateRoutes": "error",
    "onBrokenMarkdownLinks": "error",
    "themeConfig": {
      "defaultMode": "light",
      "disableSwitch": false,
      "respectPrefersColorScheme": true,
      "image": "logo.png",
      "navbar": {
        "title": "Aditya Vaidyam",
        "hideOnScroll": false,
        "items": [
          {
            "label": "Blog",
            "position": "left",
            "to": "/blog"
          },
          {
            "label": "Contact",
            "position": "right",
            "to": "/",
            "activeBaseRegex": "/([0-9a-zA-Z]+)",
            "items": [
              {
                "label": "Send an Email",
                "to": "mailto:aditya@vaidyam.me"
              },
              {
                "label": "Schedule a Meeting",
                "to": "https://calendly.com/avaidyam/meet"
              }
            ]
          }
        ]
      }
    },
    "presets": [
      [
        "@docusaurus/preset-classic",
        {
          "pages": {
            "path": "pages",
            "routeBasePath": "/"
          },
          "docs": false,
          "blog": {
            "routeBasePath": "/blog",
            "blogTitle": "Aditya Vaidyam",
            "blogDescription": "Exploring macOS Internals",
            "blogSidebarTitle": "All posts",
            "blogSidebarCount": "ALL",
            "postsPerPage": "ALL",
            "feedOptions": {
              "type": "all",
              "copyright": "Â© 2022 Aditya Vaidyam"
            }
          }
        }
      ],
      [
        "@docusaurus/plugin-client-redirects",
        {
          "fromExtensions": ["html"],
          "redirects": [
            {
              "from": "/2019/06/02/iOS_vs_macOS",
              "to": "/blog/2019/06/02/"
            },
            {
              "from": "/2019/02/19/DIY-Core-Animation",
              "to": "/blog/2019/02/19/"
            },
            {
              "from": "/2018/03/22/Exercise-Modern-Cocoa-Views",
              "to": "/blog/2018/03/22/"
            },
            {
              "from": "/2018/03/16/Building-Better-RegisterEventHotKey",
              "to": "/blog/2018/03/16/"
            },
            {
              "from": "/2018/02/22/SecretLife_CoreAnimation",
              "to": "/blog/2018/02/22/"
            },
            {
              "from": "/2018/02/19/CAStateController",
              "to": "/blog/2018/02/19/"
            },
            {
              "from": "/2018/02/18/CAPortalLayer",
              "to": "/blog/2018/02/18/"
            },
            {
              "from": "/2018/02/17/CAPluginLayer_CABackdropLayer",
              "to": "/blog/2018/02/17/"
            },
            {
              "from": "/2017/10/01/ViewBridge",
              "to": "/blog/2017/10/01/"
            },
            {
              "from": "/2016/08/05/Picture_in_Picture",
              "to": "/blog/2016/08/05/"
            },
            {
              "from": "/2016/07/12/NSExtension_PlugInKit",
              "to": "/blog/2016/07/12/"
            }
          ]
        }
      ]
    ]
  }
}
